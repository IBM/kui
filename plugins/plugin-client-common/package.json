{
  "name": "@kui-shell/plugin-client-common",
  "version": "9.3.0",
  "description": "Kui plugin that offers stylesheets",
  "license": "Apache-2.0",
  "author": "Mengting Yan",
  "homepage": "https://github.com/IBM/kui#readme",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM/kui.git"
  },
  "keywords": [
    "kui",
    "plugin"
  ],
  "main": "dist/index.js",
  "module": "mdist/index.js",
  "types": "mdist/index.d.ts",
  "dependencies": {
    "@patternfly/react-core": "4.84.4",
    "@patternfly/react-table": "4.19.45",
    "monaco-editor": "0.21.2",
    "monaco-editor-webpack-plugin": "2.1.0",
    "react-markdown": "5.0.3",
    "remark-gfm": "1.0.0",
    "spinkit": "2.0.1",
    "tmp": "0.2.1",
    "turndown": "7.0.0"
  },
  "kui": {
    "webpack": {
      "plugins": [
        {
          "plugin": "monaco-editor-webpack-plugin",
          "options": {
            "languages": [
              "css",
              "scss",
              "less",
              "dockerfile",
              "go",
              "html",
              "handlebars",
              "javascript",
              "typescript",
              "json",
              "markdown",
              "python",
              "powershell",
              "shell",
              "yaml"
            ],
            "features": [
              "!codelens",
              "!iPadShowKeyboard",
              "!multicursor",
              "!quickCommand",
              "!quickOutline",
              "!codeAction",
              "!coreCommands",
              "!fontZoom",
              "!toggleHighContrast",
              "!colorDetector"
            ]
          }
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
