{
  "name": "@kui-shell/plugin-apache-composer",
  "version": "0.6.0",
  "description": "Kui plugin for the Apache OpenWhisk Composer",
  "license": "Apache-2.0",
  "author": "Nick Mitchell",
  "homepage": "https://github.com/IBM/kui#readme",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues/new"
  },
  "repository": {
