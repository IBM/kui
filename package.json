{
  "name": "kui-shell",
  "version": "2.0.3",
  "description": "[![Travis](https://travis-ci.org/ibm-functions/shell.svg?branch=master)](https://travis-ci.org/ibm-functions/shell) [![License](https://img.shields.io/badge/license-Apache--2.0-blue.svg)](http://www.apache.org/licenses/LICENSE-2.0) [![Join Slack](https://img.shields.io/badge/join-slack-9B69A0.svg)](http://slack.openwhisk.org/) [![codecov](https://codecov.io/gh/ibm-functions/shell/branch/master/graph/badge.svg)](https://codecov.io/gh/ibm-functions/shell) [![HitCount](http://hits.dwyl.io/ibm-functions/shell.svg)](http://hits.dwyl.io/ibm-functions/shell)",
  "main": "build/app/src/main/main.js",
  "bin": {
    "kui": "bin/kui"
  },
  "files": [
    "bin/",
    "tests/",
    "dist/"
  ],
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "npx tslint 'app/src/**/*.ts' 'packages/*/src/**/*.ts' 'plugins/*/src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "npx tsc",
    "watch": "npx tsc --watch",
    "postinstall": "export CLOUDSHELL_INSTALL_UI=true; npx lerna bootstrap --hoist; (cd tests && ./bin/corral.sh && npm install) && (cd app && npm install) && npm run compile && for i in plugins/*; do if [ -d $i/node_modules ]; then (cd build/plugins/`basename $i` && rm -rf node_modules && cp -a ../../../$i/node_modules .); fi; done && (cd app && npm run prep) && (cd tests && ./bin/corral.sh)",
    "webpack-pre": "jq 'del(.dependencies.\"electron-download\")' node_modules/electron/package.json > node_modules/electron/package.json-tmp && mv node_modules/electron/package.json-tmp node_modules/electron/package.json && npm prune"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "colors": "^1.3.3",
    "columnify": "^1.5.4",
    "debug": "^4.1.1",
    "electron-context-menu": "^0.10.1",
    "electron-squirrel-startup": "^1.0.0",
    "expand-home-dir": "0.0.3",
    "fs-extra": "^7.0.1",
    "js-beautify": "^1.8.9",
    "json-colorizer": "^2.1.2",
    "mkdirp": "^0.5.1",
    "module-alias": "^2.1.0",
    "needle": "^2.2.4",
    "opn": "^5.4.0",
    "pretty-ms": "^4.0.0",
    "properties-parser": "^0.3.1",
    "tmp": "0.0.33",
    "yargs-parser": "^11.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM/kui.git"
  },
  "keywords": [
    "OpenWhisk",
    "CLI",
    "UI",
    "kubernetes",
    "visualization",
    "serverless"
  ],
  "author": {
    "name": "Nick Mitchell",
    "email": "nickm@us.ibm.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues"
  },
  "homepage": "https://github.com/IBM/kui#readme",
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/mkdirp-promise": "^5.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/swagger-schema-official": "^2.0.14",
    "@types/webdriverio": "^4.13.1",
    "css-loader": "^2.1.0",
    "electron": "^4.0.1",
    "electron-download": "^4.1.1",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "http-server": "^0.11.1",
    "ignore-loader": "^0.1.2",
    "lerna": "^3.10.5",
    "raw-loader": "^1.0.0",
    "shebang-loader": "0.0.1",
    "style-loader": "^0.23.1",
    "terser": "^3.11.0",
    "tslint": "^5.12.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2"
  },
  "_moduleAliases": {
    "@root": ".",
    "@kui": "build/app/src",
    "@kui-plugin": "build/plugins",
    "@kui-plugin-src": "plugins",
    "@settings": "app/build",
    "@prescan": "build",
    "@test": "tests"
  }
}
